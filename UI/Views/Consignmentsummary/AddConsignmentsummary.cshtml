                            <select class="form-control" id="" name="BrookerDetails">
                                <option>Direct Customer</option>
                            </select>
                            @Html.ValidationMessageFor(model => model.BrookerDetails, "", new { @class = "text-danger" })
                        </div>
                    </div>
                            @Html.HiddenFor(model => model.BillDate, new { htmlAttributes = new { @class = "form-control" } })
                           
                      

                    <div class="form-group">
                        @Html.LabelFor(model => model.ValueOfGoods, htmlAttributes: new { @class = "control-label col-md-2" })
