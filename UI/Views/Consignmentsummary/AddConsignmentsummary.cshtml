              {
                @Html.AntiForgeryToken()

                <div class="form-horizontal">
                   
                    @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                  
                            @Html.HiddenFor(model => model.ConsignorID, new { htmlAttributes = new { @class = "form-control" } })
                           
                    
                            @Html.HiddenFor(model => model.ConsigneeID, new { htmlAttributes = new { @class = "form-control" } })
                    
                    
                            @Html.HiddenFor(model => model.BookingDate, new { htmlAttributes = new { @class = "form-control" } })
                                         

                            @Html.HiddenFor(model => model.TransactionID, new { htmlAttributes = new { @class = "form-control" } })
                          

                    <div class="form-group">
                        @Html.LabelFor(model => model.BillityType, htmlAttributes: new { @class = "control-label col-md-2" })
                        <div class="col-md-10">
                            <select class="form-control" id="" name="BillityType">
                                <option>--Select--</option>
                                <option>GST INVOICE</option>
                                <option>Booking Challenge </option>
                            </select>
                            @Html.ValidationMessageFor(model => model.BillityType, "", new { @class = "text-danger" })
                        </div>
                    </div>

                    <div class="form-group">
